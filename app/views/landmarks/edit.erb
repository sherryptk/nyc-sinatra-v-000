<h1>Update <%= @landmark.name %> </h1>

<form action="/landmarks/:id" method="POST">
  <input type="hidden" name="_method" value="patch">

  <p>Name:</p>
  <input type="text" name="landmark[name]" value="landmark_name" id="name">
  <p>Update Year Completed - <%= @landmark.year_completed.to_s %> </p>
  <input type="text" name="landmark[year_completed]" value="year_completed" id="year_completed">
  <input type="submit" value="Edit Landmark" id="Edit Landmark">
</form>
